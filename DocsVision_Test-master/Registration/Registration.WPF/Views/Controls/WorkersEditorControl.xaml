<UserControl x:Class="Registration.WPF.Views.Controls.WorkersEditorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Registration.WPF.Views.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="52" d:DesignWidth="211">
    <Grid>
        <TextBox x:Name="txtWorkers" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" VerticalAlignment="Top" Width="211" Text="{Binding Path=NamesWorkers}"/>
        <ListBox x:Name="listWorkers" HorizontalAlignment="Left" Height="29" Margin="0,23,0,0" VerticalAlignment="Top" Width="211" Visibility="Hidden"/>
        <Button x:Name="btnAllWorkers" Content="..." HorizontalAlignment="Left" Height="22" Margin="184,1,0,0" VerticalAlignment="Top" Width="27" Visibility="Hidden"/>
    </Grid>
</UserControl>
